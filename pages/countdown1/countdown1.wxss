.container{
  width:100%;
  height: 500rpx;
  position: relative;
  background: url('http://pa5iwwqa4.bkt.clouddn.com/time.png');
  background-size: 100% 100%;
}
.container image{
  width:100%;
}
.time{
  color:#189adb;
  font-weight: bolder;
  position: absolute;
  font-size: 50rpx;
  top:290rpx;
}
.btn{
  width:100%;
  align-items: center;
  text-align: center;
  margin-top: 248rpx;
}
.btn .btn-item{
  width:50rpx;
  height:50rpx;
  display: inline-block;
  text-align: center;
  margin-right: 50rpx;
  line-height: 50rpx;
}
.btn .btn-item image{
  width:50rpx;
  height:50rpx;
}
