.container{
  display: flex;
  flex-direction: column;
  height: 100%;
  width:100%;
  align-items: center;
  padding:0;
  background: #f8f8f8;
  padding-top: 40rpx;
}

.item{
  width:100%;
  height: 100rpx;
  line-height: 100rpx;
  background: #fff;
  padding-left: 60rpx;
  margin-bottom: 40rpx;
}
.item image{
  width:50rpx;
  height: 50rpx;
  display: inline-block;
}
.btn{
  position: absolute;
  bottom: 0;
  width:100%;
  height: 115rpx;
  line-height: 115rpx;
  color: #fff;
  text-align: center;
  font-size: 38rpx;
  background: #46a4e2;
}

